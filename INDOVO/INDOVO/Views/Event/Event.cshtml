@model INDOVO.Models.EventDetails
    @{
        ViewBag.Title = "Event";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h1>EventID </h1>
    <h3>@Model.EventID</h3>
    <h1>Name</h1>
    <h3>@Model.Name</h3>
    <h1>Description</h1>
    <h3>@Model.Description</h3>
    <h1>ImageURL</h1>
    <img src="@Model.ImageURL" />
    <h1>Event_Start</h1>
    <h3>@Model.Event_Start</h3>
    <h1>Users</h1>
@foreach (var item in Model.Users)
{
    <h3>@item</h3>
}

